/ This will be inserted into table header div
.header.table-header{ data: { target_table: 'downloads_log'} }
  %h5
    Downloads log
.row-fluid
  .span12
    .row-group
      %table.table.table-bordered.table-hover#downloads_log
        %thead
          %tr
            %th Date
            %th Participant
            %th User
            %th Consent
        %tbody
          - @downloads.each do |download|
            %tr
              %td
                = download.date.to_date
              %td
                = download.study_involvement.participant.name
              %td
                = download.user.netid
              %td
                = link_to admin_study_involvement_download_path(download.study_involvement, download) do
                  %i.icon-file
                  = download.consent.file.filename
